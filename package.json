{
    "name": "ip_tracker",
    "jest": {
        "testMatch": [
          "<rootDir>/src/**/*.test.{js,jsx,ts,tsx}",
          "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
        ]
    },
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "^6.3.0",
        "@heroicons/react": "^1.0.4",
        "@tailwindcss/forms": "^0.3.3",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "axios": "^0.21.4",
        "leaflet": "^1.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-dotenv": "^0.1.3",
        "react-leaflet": "^3.2.1",
        "react-loader-spinner": "^4.0.0",
        "react-scripts": "4.0.3",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start": "react-dotenv && craco start",
        "build": "react-dotenv && craco build",
        "test": "craco test --watchAll=false",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "@types/leaflet": "^1.7.5",
        "autoprefixer": "^9.8.6",
        "dotenv": "^10.0.0",
        "postcss": "^7.0.36",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.14"
    },
    "react-dotenv": {
        "whitelist": "ENV_VAR_1"
    }
}
